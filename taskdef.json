{
  "executionRoleArn": "arn:aws:iam::964463549243:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "sample-website",
          "image": "<IMAGE1_NAME>",
          "essential": true,
          "memoryReservation": 512,
          "portMappings": [
              {
                  "hostPort": 80,
                  "protocol": "tcp",
                  "containerPort": 80
              }
          ]
      }
  ],
  "requiresCompatibilities": [
      "EC2"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "ecs-demo"
}
